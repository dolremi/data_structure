#include <map>
#include <iostream>

using namespace std;

bool uniqueString(string input){ 
  map<char, int> myMap;
 
  for(int i = 0; i <  input.length(); ++i){
   
    if(myMap.find(input[i]) == myMap.end()){
      myMap[input[i]] = 1;
    }
    else{    
      return FALSE;
    }
  }
  return TRUE;
}
